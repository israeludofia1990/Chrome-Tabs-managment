{
    "manifest_version": 3,
    "name": "Tab Limit Manager",
    "version": "1.0",
    "description": "Limits the number of open tabs and provides options to close or save excess tabs.",
    "permissions": ["tabs", "storage"],
    "background": {
      "service_worker": "worker.js"
    },
    "action": {
      "default_popup": "popup.html"
    },
    "options_page": "options.html",
    "icons": {
      "16": "icons/icon16.png",
      "32":	"icon/icon32.png",
      "48": "icons/icon48.png",
      "128": "icons/icon128.png"
    }
  }
  