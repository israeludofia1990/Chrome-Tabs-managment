{
    "manifest_version": 3,
    "name": "Tab Limit Manager",
    "version": "1.0",
    "description": "Limits the number of open tabs and provides options to close or save excess tabs.",
    "permissions": ["tabs", "storage", "notifications"],
    "background": {
        "service_worker": "background/worker.js"
    },
    "icons": {
        "16": "Assets/icon16.png",
        "32": "Assets/icon32.png",
        "48": "Assets/icon48.png",
        "128": "Assets/icon128.png"
    }
}
